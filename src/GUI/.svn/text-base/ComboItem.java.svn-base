package GUI;

/**
 *
 * @author rik_claessens
 */
public class ComboItem {
    private String title;
    private int id;
    public ComboItem(String title, int id){
        this.title = title;
        this.id = id;
    }

    public int getId() {
        return id;
    }

    public String getTitle() {
        return title;
    }
    
    public String toString(){
        return title;
    }
    
}
